{
    "name": "neurony/laravel-url",
    "description": "Generate custom URLs for any Eloquent model (Laravel 5.7 and up).",
    "license": "MIT",
    "keywords": [
        "laravel",
        "url",
        "custom",
        "eloquent"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\Url\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\Url\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neurony\\Url\\ServiceProvider"
            ]
        }
    }
}
